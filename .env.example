DATALAB_API_KEY=
DATALAB_API_BASE=https://www.datalab.to/api/v1
PIPELINE_MODE=datalab_api
OPENAI_API_KEY=
OPENAI_MODEL=gpt-5-nano
OPENAI_MAX_TOKENS=1024
IMAGES_DIR=images_example
OUTPUT_DIR=outputs

# ====== Google Document AI (Enterprise Document OCR) ======
# Defina PIPELINE_MODE=gdocai para ativar este modo (ou datalab_api, openai_api, chandra).
# ID do projeto Google Cloud onde o processor foi criado.
GDOC_PROJECT_ID=seu_projeto
# Região do processor (ex.: us, us-latency, eu).
GDOC_LOCATION=us
# Processor Enterprise Document OCR que executará a inferência.
GDOC_PROCESSOR_ID=xxxxxxxxxxxxxxxx
# Caminho local para o JSON da service account (Application Default Credentials).
GOOGLE_APPLICATION_CREDENTIALS=/abs/path/service-account.json
# Limiar mínimo de qualidade aceito pelo MVP (0-1).
QUALITY_MIN_SCORE=0.55
# Limiar de confiança por campo obrigatório.
FIELD_MIN_CONFIDENCE=0.75
# Ativa o quality gate do gdocai mesmo no modo datalab_api.
USE_GDOC_AI_GATE=false
